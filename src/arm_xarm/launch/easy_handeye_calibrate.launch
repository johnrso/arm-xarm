<launch>
    <include file="$(find easy_handeye)/launch/calibrate.launch">
        <arg name="eye_on_hand" value="true"/>
        <arg name="namespace_prefix" value="uarm_rs4_calibration"/>

        <arg name="freehand_robot_movement" value="true"/>

        <arg name="robot_base_frame" default="link_base" />
        <arg name="robot_effector_frame" default="link7" />
        <arg name="tracking_base_frame" default="camera_link" />
        <arg name="tracking_marker_frame" default="ar_marker_4" />
    </include>
</launch>
